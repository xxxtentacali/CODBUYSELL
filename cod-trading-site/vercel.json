{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "proofs.html",
      "use": "@vercel/static"
    },
    {
      "src": "contact.html",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/proofs.html",
      "dest": "/proofs.html"
    },
    {
      "src": "/contact.html",
      "dest": "/contact.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    }
  ]
}
